[core]
  editor = vim
	autocrlf = input

[color]
	ui = True

[merge]
	tool = vimdiff

[mergetool]
	prompt = false

[push]
	default = current

[include]
	path = ~/.git-credentials

[alias]
	find = log --pretty=\"format:%Cgreen%H %Cblue%s\" --name-status --grep
	l = log --oneline --graph

[credential]
	helper = cache --timeout=36000
[user]
	name = Ilya Yurtaev
	email = ilya.yurtaev@gmail.com
[help]
	autocorrect = 1
[http]
	postBuffer = 524288000
